  "main": "./src/service/service.js",
  "scripts": {
    "test": "npm run eslint & npm run jest",
    "jest": "jest",
    "eslint": "eslint --ext .js src/",
    "start": "concurrently --prefix \"{time} - {name}\" --names \"BACKEND, SSR\" --prefix-colors \"cyan,#23de43\" \"npm:start-backend\" \"npm:start-front-server-prod\"",
    "start::debug": "cross-env NODE_ENV=development LOG_LEVEL=debug node ./src/service/service.js --server",
    "debug": "LOG_LEVEL=debug node ./src/service/cli/server.js | ./node_modules/.bin/pino-pretty",
    "start-backend": "cross-env NODE_ENV=production LOG_LEVEL=error node ./src/service/service.js --server",
    "start-front-server": "nodemon -r dotenv/config ./express/express.js",
    "start-front-server-prod": "-r dotenv/config ./express/express.js",
    "fill-db": "cross-env NODE_ENV=development node ./service/service.js --fill-db"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rockn1989/buy-and-sell.git"
