    "eslint": "8.11.0",
    "eslint-config-htmlacademy": "0.6.0",
    "jest": "27.5.1",
    "nodemon": "2.0.15",
    "pino-pretty": "7.6.0",
    "sqlite3": "5.0.8",
    "supertest": "6.2.2"
  },
