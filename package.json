    "cross-env": "7.0.3",
    "csurf": "1.11.0",
    "dotenv": "16.0.0",
    "express": "4.17.3",
    "express-pino-logger": "7.0.0",
    "express-session": "1.17.3",
    "helmet": "5.1.0",
    "joi": "17.6.0",
